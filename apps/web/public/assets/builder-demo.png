<svg width="600" height="400" viewBox="0 0 600 400" fill="none" xmlns="http://www.w3.org/2000/svg">
  <!-- Background -->
  <rect width="600" height="400" fill="#0F172A"/>
  
  <!-- Flow Example -->
  <rect x="50" y="50" width="100" height="50" rx="8" fill="#3B82F6"/>
  <text x="100" y="80" text-anchor="middle" fill="white" font-family="Inter" font-size="12">Message</text>
  
  <rect x="200" y="50" width="100" height="50" rx="8" fill="#F59E0B"/>
  <text x="250" y="80" text-anchor="middle" fill="white" font-family="Inter" font-size="12">Contains</text>
  
  <rect x="350" y="50" width="100" height="50" rx="8" fill="#10B981"/>
  <text x="400" y="80" text-anchor="middle" fill="white" font-family="Inter" font-size="12">Reply</text>
  
  <rect x="200" y="150" width="100" height="50" rx="8" fill="#EF4444"/>
  <text x="250" y="180" text-anchor="middle" fill="white" font-family="Inter" font-size="12">Delete</text>
  
  <!-- Connections -->
  <path d="M150 75 L200 75" stroke="#60A5FA" stroke-width="2" fill="none" marker-end="url(#arrowhead)"/>
  <path d="M300 75 L350 75" stroke="#60A5FA" stroke-width="2" fill="none" marker-end="url(#arrowhead)"/>
  <path d="M250 100 L250 150" stroke="#60A5FA" stroke-width="2" fill="none" marker-end="url(#arrowhead)"/>
  
  <!-- Arrow marker -->
  <defs>
    <marker id="arrowhead" markerWidth="10" markerHeight="7" refX="10" refY="3.5" orient="auto">
      <polygon points="0 0, 10 3.5, 0 7" fill="#60A5FA"/>
    </marker>
  </defs>
  
  <!-- Title -->
  <text x="300" y="30" text-anchor="middle" fill="white" font-family="Inter" font-size="16" font-weight="600">Bot Logic Flow</text>
</svg>
