<svg width="800" height="600" viewBox="0 0 800 600" fill="none" xmlns="http://www.w3.org/2000/svg">
  <!-- Background -->
  <rect width="800" height="600" fill="#1F2937"/>
  
  <!-- Grid Pattern -->
  <defs>
    <pattern id="grid" width="40" height="40" patternUnits="userSpaceOnUse">
      <path d="M 40 0 L 0 0 0 40" fill="none" stroke="#374151" stroke-width="1"/>
    </pattern>
  </defs>
  <rect width="800" height="600" fill="url(#grid)"/>
  
  <!-- Sidebar -->
  <rect x="0" y="0" width="200" height="600" fill="#111827"/>
  <rect x="20" y="20" width="160" height="40" rx="8" fill="#3B82F6"/>
  <text x="100" y="45" text-anchor="middle" fill="white" font-family="Inter" font-size="14">Commands</text>
  
  <!-- Blocks in sidebar -->
  <rect x="20" y="80" width="160" height="30" rx="4" fill="#374151"/>
  <rect x="20" y="120" width="160" height="30" rx="4" fill="#374151"/>
  <rect x="20" y="160" width="160" height="30" rx="4" fill="#374151"/>
  
  <!-- Main Canvas -->
  <rect x="200" y="0" width="600" height="600" fill="#1F2937"/>
  
  <!-- Example Nodes -->
  <rect x="250" y="100" width="120" height="60" rx="8" fill="#8B5CF6" stroke="#A855F7" stroke-width="2"/>
  <text x="310" y="125" text-anchor="middle" fill="white" font-family="Inter" font-size="12">Ready Event</text>
  <text x="310" y="140" text-anchor="middle" fill="#E5E7EB" font-family="Inter" font-size="10">Bot startup</text>
  
  <rect x="420" y="200" width="120" height="60" rx="8" fill="#10B981" stroke="#059669" stroke-width="2"/>
  <text x="480" y="225" text-anchor="middle" fill="white" font-family="Inter" font-size="12">Send Message</text>
  <text x="480" y="240" text-anchor="middle" fill="#E5E7EB" font-family="Inter" font-size="10">Hello World!</text>
  
  <!-- Connection Line -->
  <path d="M370 130 Q390 130 420 165 Q420 180 420 200" stroke="#60A5FA" stroke-width="2" fill="none"/>
  
  <!-- Title -->
  <text x="400" y="40" text-anchor="middle" fill="white" font-family="Inter" font-size="18" font-weight="600">Visual Bot Builder</text>
</svg>
